//Define Variables - Line 1
let myName = "Marq";
let title = "Student"
let education = "Oral Rovers University";
let experience = "Tutoring, Chick Fil A, Software";
let skills = ["Running, Coding, Singing"]


//Connect Variables to the HTML
document.getElementBYId("my-name").textcontent = myName;
document.getElementBYId("title").textcontent = title;
document.getElementById("Education").textcontent = education
document.getElementBYId("Experience").textcontent = experience;
document.getElementBYId("Skills").textcontent = skills;
